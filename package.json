{
  "name": "grants-service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=8.3.0"
  },
  "scripts": {
    "start": "nodemon --exec sls offline --dontPrintOutput",
    "start-test": "sls offline --dontPrintOutput",
    "test": "eslint . && jest --detectOpenHandles --testPathIgnorePatterns compare-facets.test.js",
    "format": "prettier --write .",
    "deployTest": "sls deploy --stage=test --region=eu-west-2",
    "deployProd": "sls deploy --stage=prod --region=eu-west-2"
  },
  "eslintConfig": {
    "plugins": [
      "node"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn",
      "node/no-unpublished-require": "off",
      "node/shebang": "off",
      "strict": [
        "warn",
        "safe"
      ]
    }
  },
  "eslintIgnore": [
    ".serverless/"
  ],
  "jest": {
    "globalSetup": "./test/setup.js",
    "globalTeardown": "./test/teardown.js",
    "testEnvironment": "./test/mongo-environment.js"
  },
  "prettier": {
    "singleQuote": true,
    "quoteProps": "consistent"
  },
  "devDependencies": {
    "csv-parse": "4.10.1",
    "csv-parser": "2.3.3",
    "eslint": "7.6.0",
    "eslint-plugin-node": "11.1.0",
    "event-stream": "4.0.1",
    "jest": "26.2.2",
    "jest-environment-node": "26.3.0",
    "mongodb-memory-server": "6.7.5",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "serverless": "2.23.0",
    "serverless-domain-manager": "4.2.0",
    "serverless-offline": "6.4.0",
    "split": "1.0.1",
    "yargs": "15.4.1"
  },
  "dependencies": {
    "dictionary-cy-gb": "1.0.0",
    "dictionary-en-gb": "2.2.2",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "lodash": "4.17.19",
    "moment": "2.27.0",
    "mongodb": "3.5.9",
    "nspell": "2.1.2",
    "request": "2.88.2",
    "request-promise-native": "1.0.8",
    "server-timings": "2.0.1",
    "serverless-http": "2.5.0"
  }
}
